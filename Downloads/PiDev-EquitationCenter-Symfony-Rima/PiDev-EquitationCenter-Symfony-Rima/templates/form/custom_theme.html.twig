<script>
  function deleteActivity(userId) {
    jQuery.ajax({
      url: '/deleteUser', // URL of the Symfony controller route
      type: 'POST', // Or 'DELETE' if you prefer
      data: { id: userId }, // Data to send to the server (activity ID)
      success: function(response) {
        // Handle success response
        console.log("User with ID " + userId + " deleted successfully.");

        // Show success Toastify message

        jQuery('#row-id-' + userId).remove();
        jQuery('#cancel-button').click();
        Toastify({
          node: jQuery("#success-notification-content")
            .clone()
            .removeClass("hidden")[0],
          duration: 3000,
          newWindow: true,
          close: true,
          gravity: "top",
          position: "right",
          stopOnFocus: true,
        }).showToast();
      },
      error: function(xhr, status, error) {
        // Handle error response
        console.error("Error deleting activity:", error);

        // Show error Toastify message
        Toastify({
          node: jQuery("#failed-notification-content")
            .clone()
            .removeClass("hidden")[0],
          duration: 3000,
          newWindow: true,
          close: true,
          gravity: "top",
          position: "right",
          stopOnFocus: true,
        }).showToast();
      }
    });
  }


  // When a delete button is clicked
  jQuery('.delete-button').click(function() {
    // Retrieve the activity ID from the data attribute
    var userId = jQuery(this).data('user-id');
    console.log("User ID: " + userId);
    jQuery('#delete-confirmation-modal').addClass('show');
    jQuery('.delete-button-modal').off('click');

    jQuery('.delete-button-modal').on('click', function() {
      deleteActivity(userId);
    });
  });
</script>

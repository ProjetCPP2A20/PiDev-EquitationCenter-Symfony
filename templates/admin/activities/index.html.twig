    function selectingDefaultItem(desiredActivityType){

        const activityTypes = {
            "HORSE_RIDING_LESSONS": "Leçons d'équitation",
            "HIKES": "Randonnées",
            "HORSE_CARE_WORKSHOPS": "Ateliers de soins aux chevaux",
            "HORSE_ASSISTED_THERAPY": "Thérapie assistée par les chevaux",
            "PONY_WALKS": "Promenades en poney",
            "HORSE_TRAINING_CLINICS": "Cliniques de formation des chevaux",
            "COMPETITIVE_EVENTS": "Événements compétitifs",
            "HORSE_PENSION": "Pension pour chevaux",
            "PONY_PARTIES": "Fêtes en poney",
            "EDUCATIONAL_PROGRAMS": "Programmes éducatifs"
        };

        const controlDiv = document.getElementById('activity_typeactivity-ts-control');

        controlDiv.click();

        function findIndexByValue(value) {
            const values = Object.values(activityTypes);
            return values.indexOf(value);
        }

// Example usage:
        const index = findIndexByValue(desiredActivityType)+1;
        console.log("Index of value:", index);
        function clickOnSelectItemDiv(index) {
            const selectItemDiv = document.getElementById('activity_typeactivity-opt-' + index);
            if (selectItemDiv) {
                selectItemDiv.click();
            } else {
                // If selectItemDiv is not available, observe the document for changes
                const observer = new MutationObserver((mutationsList, observer) => {
                    for (let mutation of mutationsList) {
                        if (mutation.type === 'childList') {
                            // Check if selectItemDiv is now available
                            const updatedSelectItemDiv = document.getElementById('activity_typeactivity-opt-' + index);
                            if (updatedSelectItemDiv) {
                                // If selectItemDiv is available, click on it and disconnect the observer
                                updatedSelectItemDiv.click();
                                observer.disconnect();

                                break;
                            }
                        }
                    }
                });

                // Start observing changes in the document
                observer.observe(document.documentElement, { childList: true, subtree: true });
            }
        }

// Example usage:
        clickOnSelectItemDiv(index);
        function setDropdownCSS() {
            // Select the element with the class "ts-dropdown single plugin-dropdown_input"
            var dropdownElement = $('.ts-dropdown.single.plugin-dropdown_input');

            // Set the CSS properties to hide the element after 1 second
            setTimeout(function() {
                dropdownElement.css({
                    display: 'none',
                    visibility: 'visible'
                });

                // Log a message to indicate that the CSS properties are being set
                console.log("CSS properties set to hide the dropdown element:", dropdownElement.css('display'), dropdownElement.css('visibility'));
            }, 200); // Wait for 1 second before applying CSS changes
        }

// Call the setDropdownCSS function
        setDropdownCSS();

    }
